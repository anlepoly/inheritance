@FunctionalInterface
interface Foo {
	int foo(int x);
}

public class T {
	// Select &apos;x&apos; in lambda body and press Ctrl+G.
	Foo f1= x -&gt; x; //[1]
	Foo f2= (int x) -&gt; x; //[2]
}</thetext>
